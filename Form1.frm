' /// CREATED BY EVDEURSEN 2002
' /// a nice timer sample
' /// works only in vb5.0 and vb6.0 because of the 'addressof' function
' ////////////////////////////////////////////////////

' SETTIMER FUNCTION
Private Declare Function SetTimer Lib "user32" ( _
    ByVal hwnd As Long, _
    ByVal nIDEvent As Long, _
    ByVal uElapse As Long, _
    ByVal lpTimerFunc As Long) As Long

' KILLTIMER FUNCTION
Private Declare Function KillTimer Lib "user32" ( _
    ByVal hwnd As Long, _
    ByVal nIDEvent As Long) As Long

Private Sub Form_Load()
    Dim bEventID As Byte
    '///////////////////////////////////////////////////////////////
    ' CREATE 20 TIMEREVENTS WITH DIFFERENT INTERVALS
    ' timer1  - 0,25 secs ( 250*bEventID --> 250*1=250 (1000=1sec)
    ' timer2  - 0,50 secs ( 250*bEventID --> 250*2=500 (1000=1sec)
    ' ..
    ' ..
    ' timer19 - 4,75 secs ( 250*bEventID --> 250*19=4750 (1000=1sec)
    ' timer20 - 5,00 secs ( 250*bEventID --> 250*20=5000 (1000=1sec)
    '///////////////////////////////////////////////////////////////
    For bEventID = 1 To 20
        ' SET THE LABELS ON THE FORM
        Me.Label1(bEventID - 1).Caption = "interval:" & Format((250 * bEventID) / 1000, "0.00") & "sec"
        ' SET THE TIMERS WITH DIFFERENT INTERVALS
        Call SetTimer(Me.hwnd, bEventID, 250 * bEventID, AddressOf TimerProc)
    Next
End Sub

Private Sub Form_Unload(Cancel As Integer)
    Dim bEventID As Byte
    '///////////////////////////////////////////////////////////////
    ' EVERY TIMER THAT WAS CREATED MUST BE KILLED
    '///////////////////////////////////////////////////////////////
    For bEventID = 1 To 20
        Call KillTimer(Me.hwnd, bEventID)
    Next
End Sub
